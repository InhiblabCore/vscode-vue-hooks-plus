{
  "name": "vue-hooks-plus-view",
  "displayName": "VueHooks Plus",
  "description": "Browse VueHooks Plus resource in VS Code",
  "icon": "images/logo.png",
  "version": "1.0.0",
  "publisher": "vue-hooks-plus-view",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/InhiblabCore/vscode-vue-hooks-plus-site.git"
  },
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
		  "title": "VueHooks Plus",
		  "properties": {
			  "vue-hooks-plus-view.iframe": {
				  "type": "string",
				  "default": "https://inhiblabcore.github.io/docs/hooks",
				  "description": "Iframe URL"
			  }
		  }
    },
    "commands":[
      {
        "command": "vue-hooks-plus-view.updateURL",
        "title": "VueHooks Plus: Update your Iframe URL"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "vue-hooks-plus-view",
          "title": "VueHooks Plus",
          "icon": "images/logo.svg"
        }
      ]
    },
    "views": {
      "vue-hooks-plus-view": [
        {
          "id": "vue-hooks-plus-view.home",
          "type": "webview",
          "name": "v1.6.4"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint . --ext .ts,.tsx",
    "watch": "tsc -w -p ./"
  },
  "devDependencies": {
    "@types/download": "^8.0.2",
    "@types/express": "^4.17.14",
    "@types/node": "^12.12.0",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "eslint": "^7.1.0",
    "typescript": "^4.0.8"
  },
  "dependencies": {
    "download": "^8.0.0",
    "express": "^4.18.2",
    "http-proxy-middleware": "^2.0.6"
  }
}
